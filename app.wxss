/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: start;
  padding: 20rpx 0;
  box-sizing: border-box;
} 

page {
  background-color:rgb(66, 64, 64);
  height: 100%;
}

page::after{
  content: '';
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 0.1rpx;
  background-color: #696969;
  z-index: 9999;
}
